---
import Layout from "../../layouts/Layout.astro";
const tipo = "texto";
const autor = "Luis Duno_Gottberg";
const titulo =
  "Y nos vamos todos juntos: Migración y resistencia indígena en el contexto de la hegemonía neoliberal";
---

<Layout titulo={titulo} tipo={tipo} autor={autor}>
  <h1>{titulo}</h1>
  <p>{autor}</p>
</Layout>
